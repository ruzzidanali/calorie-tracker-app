{
  "expo": {
    "name": "CalorieTrackerApp",
    "slug": "CalorieTrackerApp",
    "scheme": "calorietracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4CAF50"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.calorietracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4CAF50"
      },
      "package": "com.ruzzidan.calorietrackerapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "calorietracker"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "backgroundColor": "#4CAF50"
    },
    "extra": {
      "eas": {
        "projectId": "d21a5689-d619-422a-b8af-5d97cebd3e91"
      }
    }
  }
}
